{
    "name": "Flask Development",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app",
    "workspaceFolder": "/app",
    "settings": {
        "python.pythonPath": "/usr/local/bin/python"
    },
    "extensions": [
        "ms-python.python"
    ],
    "forwardPorts": [
        5000,
        5001
    ],
    "postCreateCommand": "pip install -r requirements.txt"
}